{
  "manifest_version": 3,
  "name": "Calvins New Tab",
  "description": "Just my new tab",
  "version": "1.0.0",
  "icons": {
    "128": "icons/icon.png"
  },
  "action": {
    "default_icon": {
      "32": "icons/icon.png"
    },
    "default_title": "New Tab"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  }
}
